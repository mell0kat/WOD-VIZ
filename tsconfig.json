{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "noImplicitReturns": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "dist",
        "node_modules"
      ]
}
